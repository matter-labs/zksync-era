{
  "db_name": "PostgreSQL",
  "query": "\n            SELECT\n                eth_txs_history.*,\n                eth_txs.blob_sidecar,\n                eth_txs.tx_type\n            FROM\n                eth_txs_history\n            LEFT JOIN eth_txs ON eth_tx_id = eth_txs.id\n            WHERE\n                eth_txs_history.finality_status != 'finalized'\n                AND eth_txs.chain_id = $1\n            ORDER BY\n                eth_txs_history.id ASC\n            LIMIT\n                $2\n            ",
